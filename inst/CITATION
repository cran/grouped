citHeader("To cite package 'grouped' in publications use:")

citEntry(entry = "paper",
         title = "The logistic transform for bounde outcome scores",
         author = personList(as.person("E. Lesaffre"), as.person("D. Rizopoulos"), as.person("S. Tsonaka")),
         year         = 2006,
         url          = "http://www.student.kuleuven.ac.be/~m0390867/dimitris.htm",
         
         textVersion = 
         paste("Lesaffre, E., Rizopoulos, D. and Tsonaka, S. (2006)", 
               "The logistic transform for bounde outcome scores.",
               "Biostatistics (to appear)")
)